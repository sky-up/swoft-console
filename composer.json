{
  "name": "swoft-fork/console",
  "type": "library",
  "version": "v2.1.0",
  "keywords": [
    "php",
    "swoole",
    "swoft",
    "console"
  ],
  "description": "swoft command line application component",
  "homepage": "https://github.com/sky-up/swoft-console",
  "license": "Apache-2.0",
  "require": {
    "php": ">8.0",
    "ext-mbstring": "*",
    "swoft-fork/stdlib": "~2.1.0",
    "toolkit/cli-utils": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "Swoft\\Console\\": "src/"
    },
    "files": [
      "src/Helper/Functions.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "SwoftTest\\Console\\Unit\\": "test/unit/",
      "SwoftTest\\Console\\Testing\\": "test/testing/"
    }
  },
  "repositories": [
  ],
  "require-dev": {
    "swoft-fork/swoole-ide-helper": "dev-master",
    "phpunit/phpunit": "^7.5 || ^8.0"
  },
  "scripts": {
    "test": "./vendor/bin/phpunit -c phpunit.xml"
  }
}
